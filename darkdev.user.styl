    filter: invert(invert_value); max-height: logoh i
}

/*logo image invert*/
logo_invert(x) {
    invert_value = x is 1 ? 80% : 0%
    if (x == 1){
        img[class*="logo"] {filter: invert(invert_value); max-height: logoh i};
        [class*="logo"],[id*="logo"] { img {filter: invert(invert_value); max-height: logoh i}}
